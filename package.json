{
  "name": "amigos-gatsby-contentful-tailwind-starter",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby, Contentful, and TailwindCSS",
  "version": "0.1.0",
  "author": "Matthew Lopez <mathews.kyle@gmail.com>, Sharon Paz <sharon.paz.00@gmail.com>",
  "dependencies": {
    "contentful": "^7.14.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.37",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-offline": "^3.2.27",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.32",
    "gatsby-source-contentful": "^2.3.17",
    "gatsby-source-filesystem": "^2.3.28",
    "gatsby-transformer-sharp": "^2.5.14",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.7.6"
  },
  "devDependencies": {
    "contentful-import": "^7.8.6",
    "env-cmd": "^10.1.0",
    "prettier": "2.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Only-Amigos/gatsby-contentful-tailwind-starter"
  },
  "bugs": {
    "url": "https://github.com/Only-Amigos/gatsby-contentful-tailwind-starter/issues"
  }
}
